#LongestSubstringwithoutrepeatingcharacters

class Solution(object):
    def lengthOfLongestSubstring(self, s):
        """
        :type s: str
        :rtype: int
        """
        if s is None:
            return 0
        max_len = 0
        left = 0
        visited = set()

        for right in range(len(s)):
            while s[right] in visited:
                visited.remove(s[left])
                left  += 1
            visited.add(s[right])
            max_len = max(max_len, right - left + 1)
        return max_len
