#House Robber(Recursion)
class Solution:
    def rob(self, nums):
        n = len(nums)
        dp = [-1] * (n)

        def rob_from(i):
            if i >= n:
                return 0
            if dp[i] != -1:
                return dp[i]

            dp[i] = max(rob_from(i + 1), rob_from(i + 2) + nums[i])
            return dp[i]
        return rob_from(0)
